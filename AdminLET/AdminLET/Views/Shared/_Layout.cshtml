    <script>
        $(document).ready(function () {
            // Add click event listener to 'Show Employee Details' button
            $('#showModalBtn').click(function () {
                // Send AJAX request to server to get employee data
                $.ajax({
                    url: '/Home/GetEmployeeModel?', // replace with dynamic id value if needed
                    type: 'GET',
                    dataType: 'json',
                    success: function (data) {
                        // On successful response, update modal content with employee details
                        $('#employeeName').text(data.name);
                        $('#employeeDepartment').text(data.department);
                        $('#employeeSalary').text(data.salary);

                        // Show modal dialog
                        $('#employeeDetailsModal').modal('show');
                    },
                    error: function (xhr, status, error) {
                        // Handle any errors that occur during the AJAX request
                        console.log('Error:', error);
                    }
                });
            });
            // Add click event listener to 'Choose a Fruit' button
            $('#showModalBtnFruits').click(function() {
                // Send AJAX request to server to get fruit data
                $.ajax({
                    url: '/Home/GetFruits',
                    type: 'GET',
                    dataType: 'json',
                    success: function(data) {
                        // On successful response, update select dropdown options with fruit names
                        var options = '';
                
                        $.each(data, function(index, fruit) {
                            options += '<option value="' + fruit.id + '">' + fruit.name + '</option>';
                        });
                
                        $('#fruitSelect').html(options);
                
                        // Show modal dialog
                        $('#fruitModal').modal('show');
                    },
                    error: function(xhr, status, error) {
                        // Handle any errors that occur during the AJAX request
                        console.log('Error:', error);
                    }
                });
            });
            // Add change event listener to 'select1' element
            $('#fruitSelect').change(function () {
                // Get selected value from 'select1'
                var selectedValue = $(this).val();

                // Call 'GetSelectOptions' action method with selected value as parameter
                $.ajax({
                    url: '/Home/GetSelectOptions',
                    type: 'POST',
                    data: { selectedValue: selectedValue },
                    success: function(result) {
                        // Clear existing options from 'select2'
                        $('#select2').empty();
                        // Build new options from returned data and append to 'select2'
                        $.each(result, function(index, item) {
                            $('#select2').append($('<option>', {
                                value: item.value,
                                text: item.text
                            }));
                        });
                    }
                });
            });
        });
    </script>
